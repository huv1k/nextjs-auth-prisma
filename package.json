{
  "name": "nextjs-auth-prisma-template",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Lukáš Huvar",
    "email": "lukas@huvar.cz",
    "url": "https://huvik.dev/"
  },
  "scripts": {
    "build": "yarn schema:generate && yarn hooks:generate && next build",
    "dev": "next",
    "hooks:generate": "graphql-codegen --config .graphqlrc.yaml",
    "lint": "next lint",
    "postinstall": "yarn prisma generate",
    "schema:generate": "tsx src/lib/utils/build-schema.ts",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@graphql-yoga/node": "2.13.13",
    "@next-auth/prisma-adapter": "1.0.5",
    "@pothos/core": "3.22.6",
    "@pothos/plugin-prisma": "3.35.4",
    "@pothos/plugin-scope-auth": "3.14.4",
    "@prisma/client": "4.4.0",
    "graphql": "16.6.0",
    "next": "12.2.5",
    "next-auth": "4.12.3",
    "next-urql": "3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "urql": "2.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/near-operation-file-preset": "2.4.2",
    "@graphql-codegen/typed-document-node": "2.3.4",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@graphql-codegen/typescript-urql": "3.7.1",
    "@graphql-typed-document-node/core": "3.1.1",
    "@types/node": "18.8.5",
    "@types/react": "18.0.21",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "prisma": "4.4.0",
    "tsx": "3.10.1",
    "typescript": "4.8.4"
  }
}
