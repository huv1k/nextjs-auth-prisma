{
  "name": "nextjs-auth-prisma-template",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Lukáš Huvar",
    "email": "lukas@huvar.cz",
    "url": "https://huvik.dev/"
  },
  "scripts": {
    "build": "yarn schema:generate && yarn hooks:generate && next build",
    "dev": "next",
    "hooks:generate": "graphql-codegen --config .graphqlrc.yaml",
    "lint": "next lint",
    "postinstall": "yarn prisma generate",
    "schema:generate": "tsx src/lib/utils/build-schema.ts",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.5",
    "@pothos/core": "3.23.1",
    "@pothos/plugin-prisma": "3.37.0",
    "@pothos/plugin-scope-auth": "3.16.0",
    "@prisma/client": "4.7.1",
    "graphql": "16.6.0",
    "graphql-yoga": "3.1.1",
    "next": "13.0.6",
    "next-auth": "4.18.0",
    "next-urql": "3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "urql": "2.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/near-operation-file-preset": "2.4.4",
    "@graphql-codegen/typed-document-node": "2.3.9",
    "@graphql-codegen/typescript": "2.8.3",
    "@graphql-codegen/typescript-operations": "2.5.8",
    "@graphql-codegen/typescript-urql": "3.7.3",
    "@graphql-typed-document-node/core": "3.1.1",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "husky": "^8.0.2",
    "prettier": "2.8.1",
    "pretty-quick": "3.1.3",
    "prisma": "4.7.1",
    "tsx": "3.12.1",
    "typescript": "4.9.3"
  }
}
